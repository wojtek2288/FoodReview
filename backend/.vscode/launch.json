{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to FoodReview",
            "type": "coreclr",
            "request": "attach",
            "processName": "FoodReview.Api",
            "sourceFileMap": {
                "/app/code": "${workspaceRoot}"
            },
            "pipeTransport": {
                "pipeCwd": "${workspaceFolder}",
                "pipeProgram": "docker",
                "quoteArgs": false,
                "pipeArgs": [
                    "exec -i dev-api-1"
                ],
                "debuggerPath": "/vsdbg/vsdbg"
            }
        }
    ]
}